    <contact type="BOGEY" name="RIGHT_MAIN">
        <location unit="IN">
            <x>71.65</x>
            <y>98.81</y>
            <z>-116.3</z>
        </location>

        <static_friction>0.55</static_friction>
        <dynamic_friction>0.70</dynamic_friction>
        <rolling_friction>0.04</rolling_friction>
        <spring_coeff unit="LBS/FT">230000.00</spring_coeff>
