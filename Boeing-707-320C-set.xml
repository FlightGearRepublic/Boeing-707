<PropertyList>
    <sim>
        <description>Boeing 707-320C</description>

        <long-description>This is the 320C variant of the Boeing 707.
        The 320C is the most popular and most sold variant.
        It's a passenger version that can be easily converted to
        a cargo airplane.
        </long-description>

        <aircraft-version>202103071800</aircraft-version>

        <rating>
            <FDM type="int">1></FDM>
            <systems type="int">1</systems>
            <cockpit type="int">1</cockpit>
            <model type="int">1></model>
        </rating>

        <authors>
            <author n="0">
                <name>Megaf</name>
                <description>Creation</description>
                <nick>MGF87</nick>
            </author>
        </authors>

        <maintainers>
            <maintainer n="0">
                <name>Megaf</name>
                <email>mmegaf@mail.com</email>
            </maintainer>
        </maintainers>

        <primary-set type="bool">true</primary-set>
        <variant-of>Boeing-707</variant-of>

        <tags>
            <tag n="0">boeing</tag>
            <tag n="1">passenger</tag>
            <tag n="2">jet</tag>
            <tag n="3">1950s</tag>
            <tag n="4">low-wing</tag>
        </tags>

        <urls>
            <home-page>https://github.com/FlightGearRepublic/Boeing-707/wiki</home-page>
            <support>https://discord.gg/e27UuJxdtX</support>
            <wikipedia>https://en.wikipedia.org/wiki/Boeing_707</wikipedia>
            <code-repository>https://github.com/FlightGearRepublic/Boeing-707</code-repository>
        </urls>

